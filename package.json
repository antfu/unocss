{
  "name": "@unocss/monorepo",
  "version": "0.4.10",
  "private": true,
  "scripts": {
    "bench": "npm -C bench run bench",
    "build": "pnpm -r --parallel --filter !playground run build",
    "dev": "pnpm -r --parallel --filter !playground run dev",
    "lint": "eslint \"**/*.{ts,vue,mjs}\"",
    "lint:fix": "nr lint -- --fix",
    "play": "npm -C playground run dev",
    "play:build": "pnpm run build && npm -C playground run build",
    "size": "esmo scripts/size.ts",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag",
    "test": "jest",
    "test:update": "jest -u"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.10.0",
    "@antfu/ni": "^0.10.1",
    "@iconify-json/carbon": "^1.0.8",
    "@iconify-json/logos": "^1.0.1",
    "@iconify-json/mdi": "^1.0.7",
    "@iconify-json/ph": "^1.0.2",
    "@iconify-json/ri": "^1.0.1",
    "@iconify-json/tabler": "^1.0.1",
    "@iconify-json/twemoji": "^1.0.2",
    "@iconify-json/uim": "^1.0.2",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@types/ws": "^8.2.0",
    "@unocss/core": "workspace:*",
    "@unocss/preset-attributify": "workspace:*",
    "@unocss/preset-icons": "workspace:*",
    "@unocss/preset-uno": "workspace:*",
    "@unocss/reset": "workspace:*",
    "@unocss/vite": "workspace:*",
    "@vueuse/core": "^6.7.4",
    "brotli-size": "^4.0.0",
    "bumpp": "^7.1.1",
    "eslint": "^8.1.0",
    "eslint-plugin-jest": "^25.2.2",
    "esno": "^0.10.1",
    "fast-glob": "^3.2.7",
    "gzip-size": "^6.0.0",
    "jest": "^27.3.1",
    "terser": "^5.9.0",
    "ts-jest": "^27.0.7",
    "tsup": "^5.5.0",
    "typescript": "^4.4.4",
    "unocss": "workspace:*",
    "ws": "^8.2.3"
  }
}
