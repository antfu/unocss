    "@vitest/ui": "^0.5.1",
    "@vueuse/core": "^7.6.2",
    "brotli-size": "^4.0.0",
    "bumpp": "^7.1.1",
    "codemirror": "^5.65.2",
    "codemirror-theme-vars": "^0.1.1",
    "eslint": "^8.9.0",
    "esno": "^0.14.1",
    "execa": "^5.1.1",
