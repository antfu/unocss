    "@unocss/core": "link:../../packages/core",
    "@unocss/preset-icons": "link:../../packages/preset-icons",
    "@unocss/svelte-scoped": "link:../../packages/svelte-scoped",
    "marked": "^5.0.2",
    "svelte": "^3.59.1",
    "svelte-check": "^3.3.2",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4",
    "unocss": "link:../../packages/unocss",
