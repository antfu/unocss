    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.18.1",
    "@builder.io/qwik-city": "0.2.1",
    "@types/node": "latest",
    "@unocss/reset": "link:../../packages/reset",
    "typescript": "^4.9.5",
    "unocss": "link:../../packages/unocss",
    "vite": "4.1.1"
  }
}
