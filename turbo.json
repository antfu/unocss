{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "build/**"
      ]
    },
    "build-post": {
      "dependsOn": [
        "^build-post"
      ],
      "outputs": [
        "dist/**",
        "build/**"
      ]
    }
  }
}
